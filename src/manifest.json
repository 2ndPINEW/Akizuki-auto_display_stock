{
  "name": "秋月店舗情報表示",
  "description": "秋月電子通称の商品ページに店舗在庫情報を自動で表示します。",
  "version": "0.1",
  "manifest_version": 2,
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "browser_action": {
    "default_icon": "icons/16.png"
  },
  "content_scripts": [{
    "matches": ["*://akizukidenshi.com/catalog/g/*"],
    "js": ["jquery.min.js", "content_script.js"]
  }]
}